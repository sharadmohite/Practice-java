class Main {
    public static void main(String[] args) {
        int[] arr={6,9,5,3,4,2};
        for(int n:arr)
        { 
            boolean flag=true;
            for(int i=2;i<=n/2;i++)
            {
                if(n%i==0)
                {
                  flag=false;
                  break;
                }
            }
            if(flag==true)
            System.out.println(n+" is prime number");
            
            
        }
    }
}
