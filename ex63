class Test  
{
	public static void main(String[] args) 
	{
		int[] arr={6,9,10,3,5,2,4};
		
		//ascending logic 
		for(int i=0;i<arr.length;i++)
		{
			for(int j=0;j<arr.length;j++)
			{
				if(arr[i]>arr[j])
				{
					//swapping 
					int temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}

		//display 
		for(int i:arr)
		{
			System.out.print(i+" ");
		}
	}
}
